# WordPress Plugin .gitignore

# Dependencies
node_modules/
vendor/*
!vendor/freemius/

# Build artifacts
/assets/build/
/resources/assets/build/
*.min.js
*.min.css
*.map

# Exception: Include bundled vendor libraries (required for WordPress.org)
!resources/assets/vendor/**/*.min.js
!resources/assets/vendor/**/*.min.css

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS Files
.DS_Store
Thumbs.db
desktop.ini

# IDE Files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
*.swp
*.swo
*~

# Testing
/tmp/
/tests/tmp/
phpunit.xml
.phpunit.result.cache

# Composer
composer.lock
composer.phar

# Package managers
package-lock.json
yarn.lock

# Environment files
.env
.env.local
.env.*.local

# Backup files
*.bak
*.backup
*~

# Temporary files
*.tmp
.cache/

# WordPress specific
wp-config.php
.htaccess

# Distribution files (if you create zip releases)
*.zip
/releases/

# Development/temporary files at root
/*.png
/*.jpg
/*.svg
/*.html
/*.json
!package.json
!composer.json

